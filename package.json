{
    "name": "GenGo",
    "version": "0.9.1",
    "description": "GenGo - Popup LLM UI for Text Processing",
    "main": "main.js",
    "type": "module",
    "scripts": {
        "start": "electron-forge start",
        "start:win": "chcp 65001 >nul && electron-forge start",
        "start:mac": "electron-forge start",
        "dev": "electron-forge start",
        "dev:win": "chcp 65001 >nul && electron-forge start",
        "dev:mac": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "make:mac": "electron-forge make --platform=darwin",
        "make:win": "electron-forge make --platform=win32",
        "package:mac": "electron-forge package --platform=darwin",
        "package:win": "electron-forge package --platform=win32",
        "publish": "electron-forge publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TetsuakiBaba/GenGo.git"
    },
    "author": "Tetsuaki Baba",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/TetsuakiBaba/GenGo/issues"
    },
    "homepage": "https://github.com/TetsuakiBaba/GenGo",
    "devDependencies": {
        "@electron-forge/cli": "^6.4.2",
        "@electron-forge/maker-deb": "^6.4.2",
        "@electron-forge/maker-dmg": "^6.4.2",
        "@electron-forge/maker-rpm": "^6.4.2",
        "@electron-forge/maker-squirrel": "^6.4.2",
        "@electron-forge/maker-zip": "^6.4.2",
        "electron": "^28.0.0"
    },
    "keywords": [
        "japanese",
        "ime",
        "ai",
        "text-correction",
        "electron"
    ],
    "dependencies": {
        "@electron-forge/publisher-github": "^7.8.3",
        "electron-squirrel-startup": "^1.0.1",
        "i18next": "^25.4.2",
        "i18next-electron-fs-backend": "^3.0.3",
        "i18next-fs-backend": "^2.6.0",
        "update-electron-app": "^3.1.1"
    }
}